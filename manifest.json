{
	  "name": "XKCD Substitutor",
	  "description": "Changes words on the page based on XKCD comics",
	  "version": "2.0",
	  "permissions": [
	    "activeTab"
	  ],
	  "content_scripts": 
		[
			{
				"matches": ["*://*/*"],
				"js": ["substitute.js"],
				"run_at": "document_end"
			}
		],
	  "manifest_version": 2
}